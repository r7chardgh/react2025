{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/locations/:match*",
      "destination": "https://geodata.gov.hk/gs/api/v1.0.0/:match*"
    },
    {
      "source": "/locations/:match*/",
      "destination": "https://geodata.gov.hk/gs/api/v1.0.0/:match*/"
    }
  ]
}
